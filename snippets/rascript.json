{
    "For_In_Loop": {
        "prefix": "for",
        "body": [
            "for ${1:element} in ${2:iterable}",
            "{",
            "\t$0",
            "}"
        ],
        "description": "For .. In Loop"
    },
    "If_Statement": {
        "prefix": "if",
        "body": [
            "if (${1:condition})",
            "{",
            "\t$0",
            "}"
        ],
        "description": "If Statement"
    },
    "Function_Block": {
        "prefix": "function",
        "body": [
            "function ${1:name}(${2:parameters})",
            "{",
            "\t$0",
            "}"
        ],
        "description": "Function Block"
    },
    "Range_Definition": {
        "prefix": "range",
        "body": "range(${1:start}, ${2:stop}${3:, ${4:[step]}})",
        "description": "Range Definition"
    },
    "Achievement_Definition": {
        "prefix": "achievement",
        "body": [
            "achievement",
            "(",
            "\ttitle = ${1:\"${2:title}\"},",
            "\tdescription = ${3:\"${4:description}\"},",
            "\tpoints = ${5:points},",
            "\ttrigger = ${0:trigger}",
            ")"
        ],
        "description": "Achievement Definition"
    },
    "Leaderboard_Definition": {
        "prefix": "leaderboard",
        "body": [
            "leaderboard",
            "(",
            "\ttitle = ${1:\"${2:title}\"},",
            "\tdescription = ${3:\"${4:description}\"},",
            "\tstart = ${5:condition},",
            "\tcancel = ${6:condition},",
            "\tsubmit = ${7:condition},",
            "\tvalue = ${8:value}${9:,\n\tformat = ${10:[format]}}",
            ")"
        ],
        "description": "Leaderboard Definition"
    },
    "Rich_Presence_Display_Definition": {
        "prefix": "rich_presence_display",
        "body": [
            "rich_presence_display",
            "(",
            "\t${1:format_string}${2:,\n\t${3:[parameters]}}",
            ")"
        ],
        "description": "Rich Presence Display Definition"
    },
    "Rich_Presence_Value_Definition": {
        "prefix": "rich_presence_value",
        "body": "rich_presence_value(${1:name}, ${2:expression}${3:, ${4:[format]}})",
        "description": "Rich Presence Value Definition"
    },
    "Rich_Presence_Lookup_Definition": {
        "prefix": "rich_presence_lookup",
        "body": "rich_presence_lookup(${1:name}, ${2:expression}, ${3:dictionary})",
        "description": "Rich Presence Lookup Definition"
    },
    "Rich_Presence_Conditional_Display_Definition": {
        "prefix": "rich_presence_conditional_display",
        "body": [
            "rich_presence_conditional_display",
            "(",
            "\t${1:condition},",
            "\t${2:format_string}${3:,\n\t${4:[parameters]}}",
            ")"
        ],
        "description": "Rich Presence Conditional Display Definition"
    }
}