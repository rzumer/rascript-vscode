{
    "For_In_Loop": {
        "prefix": "for",
        "body": [
            "for ${1:key} in ${2:dictionary}",
            "{",
            "\t$0",
            "}"
        ],
        "description": "For .. In Loop"
    },
    "If_Statement": {
        "prefix": "if",
        "body": [
            "if (${1:condition})",
            "{",
            "\t$0",
            "}"
        ],
        "description": "If Statement"
    },
    "Function_Block": {
        "prefix": "function",
        "body": [
            "function ${1:name}(${2:parameters})",
            "{",
            "\t$0",
            "}"
        ],
        "description": "Function Block"
    },
    "Key_Value_Pair": {
        "prefix": "key",
        "body": "${1:key}: ${2:\"${3:value}\"}",
        "description": "Key-Value Pair"
    },
    "Achievement_Definition": {
        "prefix": "achievement",
        "body": [
            "achievement",
            "(",
            "\ttitle = ${1:\"${2:title}\"},",
            "\tdescription = ${3:\"${4:description}\"},",
            "\tpoints = ${5:points},",
            "\ttrigger = $0",
            ")"
        ],
        "description": "Achievement Definition"
    }
}